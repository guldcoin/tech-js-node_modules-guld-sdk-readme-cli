  },
  "keywords": [
    "guld",
    "sdk",
    "cli"
  ],
  "author": "isysd",
  "license": "MIT",
  "dependencies": {
    "commander": "^2.17.1"
  },
  "devDependencies": {
    "eslint": "^5.4.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-es5": "^1.3.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-json": "^1.2.1",
    "eslint-plugin-node": "^7.0.0",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "pre-commit": "^1.2.2",
    "chai": "^4.1.2",
    "mocha": "^5.2.0"
  },
  "pre-commit": [
    "test"
  ],
  "readme": "README.md",
  "engines": {
    "node": ">=7"
  },
  "homepage": "https://guld.tech/cli/guld-sdk-readme-cli.html",
